{
  "manifest_version": 3,
  "name": "Smart Audio EQ",
  "version": "1.0.0",
  "description": "Equalizador de audio para YouTube, Spotify Web y m√°s.",
  "action": {
    "default_popup": "popup/index.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabCapture",
    "offscreen",
    "identity"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "host_permissions": ["<all_urls>"]
}
